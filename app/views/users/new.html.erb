   

<h1>Thank You For Choosing Realtor Connection</h1>
<h2>Please Sign Up!</h2>
<%= form_for (@user) do |f| %>
    <%= f.label :username %>
    <%= f.text_field :email %>

    <%= f.label :password %>
    <%= f.password_field :password %> 

    <%= f.label :name %>
    <%= f.text_field :name %>

    <%= f.submit "Complete Sign Up" %>
<% end %>