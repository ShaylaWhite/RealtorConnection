   

<h1>Thank You For Choosing Realtor Connection</h1>
<h2>Please Sign Up!</h2>

<%= form_for(@user) do |f| %> 
    <%= f.label :email %>
    <%= f.email_field :email, required:true %>
    <br>
    <br>
    <%= f.label :password %>
    <%= f.password_field :password %> 
    <br>
    <br>
    <%= f.label :name %>
    <%= f.text_field :name %>
    <br>
    <br>
    <%= f.submit "Complete Sign Up" %>
<% end %>